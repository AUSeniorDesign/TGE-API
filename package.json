{
  "name": "tgeapireact",
  "version": "1.0.0",
  "description": "A server architecture for The Great Escape's e-Commerce",
  "private": true,
  "engines": {
    "node": "^6.9.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "debug": "node --debug server/server.js",
    "inspect": "node --inspect-brk server/server.js",
    "reset": "node server/server.js reset",
    "test": "nyc mocha",
    "dev": "nf --procfile Procfile-dev --port 3000 start",
    "build": "webpack --progress --config webpack.prod.js",
    "postinstall": "npm run build",
    "client-reload": "webpack-dev-server --history-api-fallback --progress --inline --config webpack.dev-standalone.js",
    "server-reload": "nodemon server/server.js",
    "document": "docco server/routes/*.js",
    "restart": "npm run build && npm run start"
  },
  "dependencies": {
    "acorn": "^5.3.0",
    "appmetrics": "^3.1.3",
    "appmetrics-dash": "^3.4.0",
    "appmetrics-prometheus": "^1.0.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "connect-session-sequelize": "^5.2.0",
    "cookie-parser": "^1.4.3",
    "csv-reader": "^1.0.1",
    "express": "^4.15.3",
    "express-session": "^1.15.6",
    "ibm-cloud-env": "^0",
    "log4js": "^2.5.3",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql2": "^1.5.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-facebook-token": "^3.3.0",
    "passport-local": "^1.0.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.31.2",
    "sequelize-noupdate-attributes": "^1.0.0",
    "sqlite3": "^3.1.13",
    "xml2json": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "bulma": "^0.6.2",
    "chai": "^4.0.0",
    "css-loader": "^0.28.7",
    "foreman": "^2.0.0",
    "html-webpack-plugin": "^3.0.6",
    "mocha": "^5.0.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "nyc": "^11.4.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^2.7.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.1"
  }
}
